{
    "name": "eppz-code",
    "displayName": "eppz! Code",
    "description": "Visual Studio Code colors with a sense.",
    "version": "0.0.1",
    "publisher": "eppz",
    "engines":
    {
        "vscode": "^1.10.0"
    },
    "categories":
    [
        "Languages",
        "Themes"
    ],
    "contributes":
    {
        "languages":
        [
            {
                "id": "cs.original",
                "aliases": [ "csharp.original" ],
                "extensions": [ ".cs.original" ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "cs"
            }
        ],
        "grammars":
        [
            {
                "language": "cs.original",
                "scopeName": "source.cs.original",
                "path": "./syntaxes/csharp.original.tmLanguage"
            },
            {
                "injectTo": [ "source.cs.original" ],
                "scopeName": "source.injections",
                "path": "./syntaxes/csharp.injections.tmLanguage.json"
            },
            {
                "language": "cs",
                "scopeName": "source.cs",
                "path": "./syntaxes/csharp.tmLanguage.json"
            }
        ],
        "themes":
        [
            {
                "label": "eppz! Code",
                "uiTheme": "vs-dark",
                "path": "./themes/eppz-code.json"
            }
        ]
    }
}