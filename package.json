{
    "name": "eppz-code",
    "displayName": "eppz! Code",
    "description": "Visual Studio Code colors with a sense.",
    "version": "0.0.1",
    "publisher": "eppz",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Languages",
        "Themes"
    ],
    "contributes": {
        "languages": [
            {
                "id": "cs"
            },
            {
                "id": "cs_",
                "aliases": [ "csharp_" ],
                "extensions": [ ".cs_" ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
        {
            "language": "cs",
            "scopeName": "source.cs",
            "path": "./syntaxes/csharp.unity.tmLanguage.json"
        },
        {
            "language": "cs_",
            "scopeName": "source.cs_",
            "path": "./syntaxes/csharp.tmLanguage"
        }],
        "themes": [
            {
                "label": "eppz! Code",
                "uiTheme": "vs-dark",
                "path": "./themes/eppz-code.json"
            }
        ]
    }
}