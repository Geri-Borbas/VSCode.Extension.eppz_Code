{
    "name": "eppz-code",
    "displayName": "eppz! Code",
    "description": "Visual Studio Code colors with a sense.",
    "version": "0.0.1",
    "publisher": "eppz",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Languages",
        "Themes"
    ],
    "contributes": {
        "languages": [
            {
                "id": "cs"
            },
            {
                "id": "cs.original",
                "aliases": [ "csharp.original" ],
                "extensions": [ ".cs.original" ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
        {
            "language": "cs",
            "scopeName": "source.cs",
            "path": "./syntaxes/csharp.tmLanguage.json"
        },
        {
            "language": "cs.original",
            "scopeName": "source.cs.original",
            "path": "./syntaxes/csharp.original.tmLanguage"
        }],
        "themes": [
            {
                "label": "eppz! Code",
                "uiTheme": "vs-dark",
                "path": "./themes/eppz-code.json"
            }
        ]
    }
}