		},
		{
			"name": "Types (UnityEditor)",
			"scope": [ "regions.namespace.declaration support.type.unity.UnityEditor" ],
			"settings": { "foreground": "#748082" } // - 1
		},
		{
			"name": "Types (System)",
			"scope": [ "regions.namespace.declaration support.type.System" ],
			"settings": { "foreground": "#637174" } // - 2
